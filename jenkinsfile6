pipeline{
    agent any
    parameters{
        string(name: 'fName', defaultValue: 'Shon', description: ' ')
        choice(name: 'City', choices: ['one', 'two', 'three'], decription: '')
    }
    
      stages {
   
        stage('one'){
        
            steps{
                echo params.fName
            }
        
        
        }
   
   
   
   }













}
