pipeline{
    agent any
    environment{
         abc = 'Hello World'
    }
    
    stages{
    
        stage('one'){
        
            when {
                 environment name: 'abc', value: 'Value'
            
            }
        
            steps{
                echo 'Value has been passed'
            
            }
        
        
        }
    
    
    
    
    
    }













}
