pipeline{
    agent any
    environment{
          fname = "Shon"
    }
    
    stages{
        stage('one'){
           steps{
           
               script{
                   try{
                      sh 'abcefg'
                   }
                   
                   catch(e){
                       echo "Error has occured"
                   }
            }
           
           }
           
           
           }
        
        
        
        }
  }  
    
    
    
    
    
    















